ttom > elmtBottom && page < MAX_PAGES){\r\n                setIsLoaded(false);\r\n                return \r\n            }\r\n        }\r\n    }\r\n            \r\n    /*effect*/\r\n\r\n    useEffect(() => {\r\n        if(isLoaded === false){\r\n            setIsLoaded(true);\r\n        }\r\n\r\n    },[datas])\r\n\r\n    useEffect(() => {\r\n\r\n        if(isLoaded === false && page < MAX_PAGES){\r\n            setPage();\r\n        }\r\n\r\n        if(page === MAX_PAGES){\r\n            setIsLoaded(true);\r\n        }\r\n\r\n    }, [isLoaded]);\r\n\r\n    useEffect(() => {\r\n        window.addEventListener('scroll',handleScroll, true);\r\n        return window.addEventListener('scroll',handleScroll);\r\n    });\r\n\r\n    return (\r\n        <>  \r\n            {children}\r\n            <LoaderList classes={!isLoaded ? loaderListClasses : \"d-none\"} url={loaderList}/>\r\n        </>\r\n    )\r\n};\r\n\r\nexport default InfiniteListThree;\r\n","C:\\Users\\pablo\\Desktop\\seven-fallen-app\\src\\components\\Cards__toolbox\\CardsCounter.jsx",[],"C:\\Users\\pablo\\Desktop\\seven-fallen-app\\src\\api\\HolyBook.jsx",[],"C:\\Users\\pablo\\Desktop\\seven-fallen-app\\src\\api\\Authentication.jsx",[],"C:\\Users\\pablo\\Desktop\\seven-fallen-app\\src\\hooks\\useLocalStorage.js",[],"C:\\Users\\pablo\\Desktop\\seven-fallen-app\\src\\contexts\\AuthContext.jsx",["373","374"],"import {createContext, useState, useEffect} from 'react';\r\n\r\n\r\nconst AuthContext = createContext([null, () => {}]);\r\n\r\nfunction AuthContextProvider(props){\r\n    const {callback} = props;\r\n    const [isAuthenticated, setisAuthenticated] = useState();\r\n\r\n    useEffect(async () => {\r\n        let response = await callback();\r\n        setisAuthenticated(response.isAuthenticated)   \r\n    },[])\r\n\r\n    return (\r\n        <AuthContext.Provider value={[isAuthenticated, setisAuthenticated]}>\r\n            {props.children}\r\n        </AuthContext.Provider>\r\n    )\r\n}\r\n\r\nexport {Aut